     </classes>
     </test>
     <test thread-count="5" name="Validating Navigation Bar">
    <classes>
      <class name="SeleniumFramework.validateNavigationBar"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
