<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
<listeners>
<listener class-name="SeleniumFramework.listeners"/>
</listeners>

  <test thread-count="5" name="Validating User Login">
    <classes>
      <class name="SeleniumFramework.HomePage"/>
     </classes>
      </test>
      <test thread-count="5" name="Validating Title">
